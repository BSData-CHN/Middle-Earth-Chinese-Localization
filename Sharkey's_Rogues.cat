            </rule>
            <rule id="622a-e287-9f5d-218e" name="Whoever Strikes me shall be Accursed (Passive)" book="Armies of the Lord of the Rings" page="209" hidden="false">
              <profiles/>
              <rules/>
              <infoLinks/>
              <modifiers/>
              <description>
